<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>method</key>
    <string>ad-hoc</string>  <!-- Distribution method: ad-hoc, app-store, enterprise, development -->
    <key>teamID</key>
    <string>YOUR_TEAM_ID</string>  <!-- Your Apple Developer Team ID -->
    <key>signingStyle</key>
    <string>manual</string>  <!-- Use manual signing -->
    <key>provisioningProfiles</key>
    <dict>
        <key>com.your.bundle.id</key>  <!-- Your app's bundle ID -->
        <string>Your_Provisioning_Profile_Name</string>  <!-- Name of your provisioning profile -->
    </dict>
</dict>
</plist>
